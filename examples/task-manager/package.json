    "@vertz/core": "workspace:*",
    "@vertz/ui-server": "workspace:*"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@vertz/ui-compiler": "workspace:*",
    "bun-types": "^1.3.9",
    "happy-dom": "^20.6.1",
    "typescript": "^5.8.0",
    "vite": "^6.0.0"
  }
}
