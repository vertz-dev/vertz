{
  "name": "@vertz-examples/task-manager",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./jsx-runtime": "./src/jsx-runtime.ts",
    "./jsx-dev-runtime": "./src/jsx-runtime.ts"
  },
  "scripts": {
    "dev": "vite",
    "dev:ssr": "bun run src/server.ts",
